<DModal class="assign" @title={{this.title}} @closeModal={{@closeModal}}>
  <:body>
    <TopicAssignments @assignments={{this.assignments}} />
  </:body>
  <:footer>
    <DButton
      class="btn-primary"
      @action={{this.submit}}
      @label={{if
        this.model.reassign
        "discourse_assign.reassign.title"
        "discourse_assign.assign_modal.assign"
      }}
    />

    <DModalCancel @close={{@closeModal}} />
  </:footer>
</DModal>